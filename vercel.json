{
  "version": 2,
  "name": "modus-pampa-v3",
  "buildCommand": "chmod +x install.sh && ./install.sh && flutter build web --release",
  "outputDirectory": "build/web",
  "installCommand": "echo 'Using custom install script'",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}